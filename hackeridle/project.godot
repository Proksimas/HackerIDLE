; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="HackerIDLE"
run/main_scene="res://Game/main.tscn"
config/features=PackedStringArray("4.3", "GL Compatibility")
config/icon="res://icon.svg"

[autoload]

Global="*res://Game/OthersSingleton/global.tscn"
Player="*res://Game/OthersSingleton/player.gd"
LearningItemsDB="*res://Game/DB/learning_items_db.tscn"
Calculs="*res://Game/OthersSingleton/calculs.tscn"
HackingItemsDb="*res://Game/DB/hacking_items_db.tscn"
SourcesDb="*res://Game/DB/sources_db.tscn"
SkillsManager="*res://Game/Skills/skills_manager.tscn"
StatsManager="*res://Game/OthersSingleton/stats_manager.tscn"
Save="*res://Game/Saves/Save.tscn"
TimeManager="*res://Game/OthersSingleton/time_manager.tscn"
EventsManager="*res://Game/OthersSingleton/events_manager.tscn"

[display]

window/size/viewport_width=590
window/size/viewport_height=1278
window/stretch/mode="canvas_items"
window/handheld/orientation=1

[global_group]

savable=""

[internationalization]

locale/translations=PackedStringArray("res://Game/Translations/generic_translations.en.translation", "res://Game/Translations/generic_translations.fr.translation", "res://Game/Translations/Skills/skills_translations.en.translation", "res://Game/Translations/Skills/skills_translations.fr.translation", "res://Game/Translations/learning_items.en.translation", "res://Game/Translations/learning_items.fr.translation", "res://Game/Translations/Infamy/infamy_desc.en.translation", "res://Game/Translations/Infamy/infamy_desc.fr.translation", "res://Game/News/TextFiles/breaking_news.en.translation", "res://Game/News/TextFiles/breaking_news.fr.translation", "res://Game/News/TextFiles/chronological_news.en.translation", "res://Game/News/TextFiles/chronological_news.fr.translation", "res://Game/News/TextFiles/random_news.en.translation", "res://Game/News/TextFiles/random_news.fr.translation", "res://Game/News/TextFiles/player_milestone.en.translation", "res://Game/News/TextFiles/player_milestone.fr.translation", "res://Game/Events/events_trans.en.translation", "res://Game/Events/events_trans.fr.translation", "res://Game/Translations/Scenarios/conclusion.en.translation", "res://Game/Translations/Scenarios/conclusion.fr.translation", "res://Game/Translations/Scenarios/introduction.en.translation", "res://Game/Translations/Scenarios/introduction.fr.translation", "res://Game/Translations/hack_items.en.translation", "res://Game/Translations/hack_items.fr.translation")

[rendering]

renderer/rendering_method="gl_compatibility"
